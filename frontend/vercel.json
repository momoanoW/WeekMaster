{
  "version": 2,
  "buildCommand": "npm run build:staging",
  "outputDirectory": "dist/frontend/browser",
  "framework": "angular",
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
